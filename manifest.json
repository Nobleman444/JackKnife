{
    "manifest_version" : 2,
    "name" : "JackKnife",
    "version" : "0.2",
    
    "description" : "Adds JKIndex and the other JK files to web pages",
    
    "icons" : {
        "48" : "JKInfo/JackKnife-48.png"
    },
    
    "content_scripts" : [{
        "matches" : ["*://*/*"],
        "js" : ["JKAgent.js"]
    }],
    
    "web_accessible_resources" : [
        "JKIndex.js"
    ]
}